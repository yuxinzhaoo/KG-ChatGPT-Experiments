            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.example.org/kitchen#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.example.org/kitchen#Drawers"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://www.example.org/kitchen#Toaster">

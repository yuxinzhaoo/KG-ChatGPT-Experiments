        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="http://www.example.org/kitchen#Blender">
        <rdfs:subClassOf rdf:resource="http://www.example.org/kitchen#KitchenObject"/>
        <rdfs:subClassOf>
            <owl:Restriction>
